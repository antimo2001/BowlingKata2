{
  "name": "bowlingkata2",
  "version": "0.0.1",
  "description": "practice typescript with mocha tests",
  "main": "index.js",
  "dependencies": {
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json && npm test > out/build.out",
    "mocha-help": "mocha --help",
    "tsc": "tsc",
    "tsc-build": "tsc --build tsconfig.json",
    "test0": "mocha -r ts-node/register test/hello-world.test.ts",
    "test-watch": "mocha  --watch  -r ts-node/register  test/**/*.test.ts",
    "test-report": "mocha -r ts-node/register  test/**/*.test.ts  --reporter spec > out/test-report.txt",
    "test": "mocha -r ts-node/register  test/**/*.test.ts"
  },
