{
  "name": "bowlingkata2",
  "version": "0.0.6",
  "description": "practice typescript with mocha tests",
  "main": "index.js",
  "dependencies": {
    "debug": "^4.1.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1"
  },
  "scripts": {
    "pre-build": "mkdir out",
    "build": "npm run build-default",
    "build-default": "npm run tsc-build > out/build.log &&  npm run test-default >> out/build.log",
    "mocha-help": "mocha --help > out/mocha-help.out",
    "mocha-reporters": "mocha --reporters > out/mocha-reporters.out",
    "tsc-repl": "ts-node",
    "tsc": "tsc",
    "tsc-build": "tsc --build tsconfig.json",
    "test": "npm run test-watch",
    "test-fip": "SET DEBUG=fip01:* &&  mocha -w -r ts-node/register test/**/*.test.ts --reporter spec",
    "test0": "mocha -r ts-node/register test/hello-world.test.ts",
    "test1": "SET DEBUG=fip01:* &&  mocha -w -r ts-node/register test/BowlingGame.test.ts",
    "test-report": "mocha -r ts-node/register test/**/*.test.ts -R landing",
    "test-report1": "mocha -r ts-node/register test/**/*.test.ts -R progress",
    "test-report2": "mocha -r ts-node/register test/**/*.test.ts -R min",
    "test-watch": "mocha -w -r ts-node/register test/**/*.test.ts --reporter dot",
    "test-default": "mocha -r ts-node/register test/**/*.test.ts --reporter dot"
  },
  "author": "",
  "license": "ISC"
}
