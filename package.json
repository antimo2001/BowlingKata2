{
  "name": "bowlingkata2",
  "version": "0.0.4",
  "description": "practice typescript with mocha tests",
  "main": "index.js",
  "dependencies": {
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "debug": "^4.1.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1"
  },
  "scripts": {
    "pre-build": "mkdir out",
    "goto-f-drive": "F:",
    "build": "npm run tsc-build > out/build01.log  &&  npm test > out/build02.log",
    "mocha-help": "mocha --help > out/mocha-help.out",
    "tsc-repl": "ts-node",
    "tsc": "tsc",
    "tsc-build": "tsc --build tsconfig.json",
    "test-fip": "SET DEBUG=fip01:*  &&  mocha -w -r ts-node/register test/**/*.test.ts",
    "test0": "mocha -r ts-node/register test/hello-world.test.ts",
    "test1": "SET DEBUG=fip01:*  &&  mocha -w -r ts-node/register test/Frame.test.ts",
    "test-watch": "mocha  -w  -r ts-node/register  test/**/*.test.ts",
    "test-report": "mocha -r ts-node/register  test/**/*.test.ts  --reporter spec",
    "test": "mocha -r ts-node/register  test/**/*.test.ts"
  },
  "author": "",
  "license": "ISC"
}
